{
  "name": "parcial",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p .",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "migrate:dev": "prisma migrate dev --name init --preview-feature || true",
    "wait-for-db": "node ./scripts/wait-for-db.js",
    "seed": "node ./prisma/seed.js",
    "lint": "eslint . --ext .ts",
    "test": "jest --runInBand",
    "test:ci": "npm run prisma:generate && jest --runInBand --ci",
    "docker:build": "docker build -t parcial-app ."
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.0.0",
    "@types/supertest": "^2.0.12",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-security": "^1.5.0",
    "jest": "^29.0.0",
    "prettier": "^2.8.8",
    "prisma": "^5.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
